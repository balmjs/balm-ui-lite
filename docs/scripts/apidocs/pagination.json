{
  "props": [{
    "name": "page",
    "type": "Number",
    "default": 1,
    "description": "当前页数"
  }, {
    "name": "recordCount",
    "type": "Number",
    "default": "(required)",
    "description": "数据总数",
    "required": true
  }, {
    "name": "pageSize",
    "type": "Number",
    "default": "(required)",
    "description": "每页条数",
    "required": true
  }, {
    "name": "prev",
    "type": "String",
    "default": "",
    "description": "上一页"
  }, {
    "name": "next",
    "type": "String",
    "default": "",
    "description": "下一页"
  }, {
    "name": "pageSpan",
    "type": "Number",
    "default": 3,
    "description": "分页跨度"
  }, {
    "name": "showRecord",
    "type": "Boolean",
    "default": false,
    "description": "显示分页记录"
  }, {
    "name": "showJumper",
    "type": "Boolean",
    "default": false,
    "description": "显示跳转信息"
  }, {
    "name": "jumperBefore",
    "type": "String",
    "default": "'Goto'",
    "description": ""
  }, {
    "name": "jumperAfter",
    "type": "String",
    "default": "",
    "description": ""
  }, {
    "name": "jumperButton",
    "type": "String",
    "default": "",
    "description": ""
  }, {
    "name": "position",
    "type": "String",
    "default": "",
    "description": "分页对齐方式（'left' 或 'right'）"
  }, {
    "name": "mini",
    "type": "Boolean",
    "default": false,
    "description": "使用极简模式"
  }],
  "slots": [{
    "name": "(default)",
    "description": "默认插槽可以插入分页额外内容",
    "props": "{ recordCount, pageSize, pageCount }"
  }],
  "events": [{
    "name": "change",
    "description": "切换分页事件"
  }]
}
