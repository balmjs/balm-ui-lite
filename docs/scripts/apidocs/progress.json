{
  "props": [
    {
      "name": "active",
      "type": "Boolean",
      "default": "false",
      "description": "激活进度状态（无进度条和缓冲区）"
    },
    {
      "name": "progress",
      "type": "Number, String",
      "default": "0",
      "description": "当前进度条的进度（深色部分） [0-100]"
    },
    {
      "name": "buffer",
      "type": "Number, String",
      "default": "0",
      "description": "当前进度条的缓冲区（浅色部分） [0-100]"
    }
  ],
  "sass": [
    {
      "var": "$progress-main-color",
      "description": "当前进度的颜色"
    },
    {
      "var": "$progress-secondary-color",
      "description": "进度条的颜色"
    }
  ]
}
