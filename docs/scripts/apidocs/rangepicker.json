{
  "props": [
    {
      "name": "model (v-model)",
      "type": "null",
      "description": "输入框内容"
    },
    {
      "name": "config",
      "type": "Object",
      "default": "{}",
      "description": "同 Datepicker，日期选择配置，请参照 [<a href='https://chmln.github.io/flatpickr/options/' target='_blank'>flatpickr文档</a>]"
    },
    {
      "name": "startPlaceholder",
      "type": "String",
      "default": "",
      "description": "开始时间的 placeholder 属性"
    },
    {
      "name": "endPlaceholder",
      "type": "String",
      "default": "",
      "description": "结束时间的 placeholder 属性"
    },
    {
      "name": "options",
      "type": "Array",
      "default": "[]",
      "description": "快速选取时间范围选项（详见下面的使用注意点）"
    },
    {
      "name": "reverseSelection",
      "type": "Boolean",
      "default": "false",
      "description": "反向选择日期范围（反向选择：选择范围时，结束时间不变，开始时间变化；默认正向选择：选择范围时，开始时间不变，结束时间变化）"
    }
  ],
  "slots": [
    {
      "name": "separator",
      "description": "Separator插槽可以插入自定义日期分隔符"
    }
  ],
  "events": [
    {
      "name": "change",
      "type": "Function(value: array)",
      "description": "当日期变化时触发函数"
    }
  ],
  "sass": [
    {
      "var": "$rangepicker-input-width",
      "description": "日期范围输入区域width属性"
    },
    {
      "var": "$rangepicker-input-padding",
      "description": "日期范围输入区域padding属性"
    },
    {
      "var": "$rangepicker-input-border-color",
      "description": "日期范围输入区域边框颜色"
    },
    {
      "var": "$rangepicker-input-hover-border-color",
      "description": "日期范围输入区域hover时的边框颜色"
    },
    {
      "var": "$rangepicker-textfield-width",
      "description": "日期范围输入框的width属性"
    },
    {
      "var": "$rangepicker-tabs-border-width",
      "description": "日期选择区域width属性"
    },
    {
      "var": "$rangepicker-tabs-border-color",
      "description": "日期选择区域边框颜色"
    },
    {
      "var": "$rangepicker-tab-active-color",
      "description": "日期选择激活时的前景色"
    },
    {
      "var": "$rangepicker-tab-active-bg-color",
      "description": "日期选择激活时的背景色"
    },
    {
      "var": "$rangepicker-tab-line-height",
      "description": "日期选择的行高"
    },
    {
      "var": "$rangepicker-tab-hover-color",
      "description": "日期选择hover时的前景色"
    }
  ]
}
